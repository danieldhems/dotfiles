
export PATH=/Applications/SenchaSDKTools-2.0.0-beta3:$PATH

#Add Sublime to path
export PATH=$PATH:/Applications/Sublime\ Text.app/Contents/SharedSupport/bin

#Add Apache Maven to path
export PATH=~/bin/apache-maven-3.2.1/apache-maven/src/bin:$PATH

#Add Sencha CMD to path
export PATH=$PATH:~/bin/Sencha/Cmd/4.0.4.84

#Add Ruby to path
export PATH=$PATH:/usr/bin/ruby

#Add Sass to path
export PATH=$PATH:/usr/bin/sass

#Add mysql to path
export PATH=$PATH:/Applications/MAMP/Library/bin/mysql

#Set sublime as SVN editor
export SVN_EDITOR=vim

export SENCHA_SDK_TOOLS_2_0_0_BETA3="/Applications/SenchaSDKTools-2.0.0-beta3"

export dwm='https://source.keytree.net/keytree/JaguarLandRover/DealerWarrantyManagement'
export grp='https://source.keytree.net/keytree/JaguarLandRover/DCP/frontend_2014/'

export JAVA_HOME=$(/usr/libexec/java_home)
export PATH=$JAVA_HOME/jre/bin:$PATH

export M3_HOME='~/bin/apache-maven-3.2.1'

alias chrome_cors='open Google\ Chrome -a --args --disable-web-security'

# Git autocomplete
if [ -f ~/.git-completion.bash ]; then
  . ~/.git-completion.bash
fi

